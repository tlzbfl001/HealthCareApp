<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:app="http://schemas.android.com/apk/res-auto"
   android:layout_width="match_parent"
   android:layout_height="wrap_content"
   android:background="@color/transparent">
   <androidx.constraintlayout.widget.ConstraintLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginTop="67dp"
      android:layout_marginLeft="25dp"
      android:layout_marginRight="25dp"
      android:background="@drawable/rec_main_calendar"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent">
      <androidx.constraintlayout.widget.ConstraintLayout
         android:id="@+id/clCalTop"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:background="#729FFF"
         android:paddingTop="6dp"
         android:paddingBottom="6dp"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintEnd_toEndOf="parent">
         <ImageView
            android:id="@+id/btnCalPrev"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:src="@drawable/arrow_left_round"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/tvCalTitle"
            app:layout_constraintHorizontal_weight="1"/>
         <TextView
            android:id="@+id/tvCalTitle"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="2023  June"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="24dp"
            android:textStyle="bold"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/btnCalPrev"
            app:layout_constraintEnd_toStartOf="@id/btnCalNext"
            app:layout_constraintHorizontal_weight="7"/>
         <ImageView
            android:id="@+id/btnCalNext"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:src="@drawable/arrow_right_round"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/tvCalTitle"
            app:layout_constraintHorizontal_weight="1"/>
      </androidx.constraintlayout.widget.ConstraintLayout>
      <androidx.constraintlayout.widget.ConstraintLayout
         android:id="@+id/clCalBottom"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:paddingLeft="15dp"
         android:paddingRight="15dp"
         app:layout_constraintTop_toBottomOf="@id/clCalTop"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintEnd_toEndOf="parent">
         <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/calWeek"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">
            <TextView
               android:id="@+id/tvSun"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:text="SUN"
               android:textAlignment="center"
               android:textColor="#5A8EFF"
               android:textSize="11dp"
               android:textStyle="bold"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintEnd_toStartOf="@id/tvMon"
               app:layout_constraintHorizontal_weight="1"/>
            <TextView
               android:id="@+id/tvMon"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:text="MON"
               android:textAlignment="center"
               android:textColor="#5A8EFF"
               android:textSize="11dp"
               android:textStyle="bold"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/tvSun"
               app:layout_constraintEnd_toStartOf="@+id/tvTue"
               app:layout_constraintHorizontal_weight="1"/>
            <TextView
               android:id="@+id/tvTue"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:text="TUE"
               android:textAlignment="center"
               android:textColor="#5A8EFF"
               android:textSize="11dp"
               android:textStyle="bold"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/tvMon"
               app:layout_constraintEnd_toStartOf="@id/tvWed"
               app:layout_constraintHorizontal_weight="1"/>
            <TextView
               android:id="@+id/tvWed"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:text="WED"
               android:textAlignment="center"
               android:textColor="#5A8EFF"
               android:textSize="11dp"
               android:textStyle="bold"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/tvTue"
               app:layout_constraintEnd_toStartOf="@id/tvThu"
               app:layout_constraintHorizontal_weight="1"/>
            <TextView
               android:id="@+id/tvThu"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:text="THU"
               android:textAlignment="center"
               android:textColor="#5A8EFF"
               android:textSize="11dp"
               android:textStyle="bold"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/tvWed"
               app:layout_constraintEnd_toStartOf="@id/tvFri"
               app:layout_constraintHorizontal_weight="1"/>
            <TextView
               android:id="@+id/tvFri"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:text="FRI"
               android:textAlignment="center"
               android:textColor="#5A8EFF"
               android:textSize="11dp"
               android:textStyle="bold"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/tvThu"
               app:layout_constraintEnd_toStartOf="@id/tvSat"
               app:layout_constraintHorizontal_weight="1"/>
            <TextView
               android:id="@+id/tvSat"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:text="SAT"
               android:textAlignment="center"
               android:textColor="#5A8EFF"
               android:textSize="11dp"
               android:textStyle="bold"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/tvFri"
               app:layout_constraintEnd_toEndOf="parent"
               app:layout_constraintHorizontal_weight="1"/>
         </androidx.constraintlayout.widget.ConstraintLayout>
         <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/calWeek" />
      </androidx.constraintlayout.widget.ConstraintLayout>
      <androidx.constraintlayout.widget.ConstraintLayout
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:orientation="vertical"
         app:layout_constraintTop_toBottomOf="@id/clCalBottom"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintEnd_toEndOf="parent"
         app:layout_constraintBottom_toBottomOf="parent">
         <View
            android:id="@+id/view"
            android:layout_width="match_parent"
            android:layout_height="0.8dp"
            android:background="#B1B1B1"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
         <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cvList"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/view">
            <ImageView
               android:id="@+id/ivFood"
               android:layout_width="6dp"
               android:layout_height="6dp"
               android:src="@drawable/oval"
               android:layout_marginLeft="5dp"
               app:tint="#E1515F"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"/>
            <TextView
               android:id="@+id/tvFood"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginLeft="3dp"
               android:text="식단"
               android:textSize="12dp"
               android:textColor="@color/black"
               app:layout_constraintStart_toEndOf="@id/ivFood"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"/>
            <ImageView
               android:id="@+id/ivWater"
               android:layout_width="6dp"
               android:layout_height="6dp"
               android:src="@drawable/oval"
               android:layout_marginLeft="7dp"
               app:tint="#41C6FF"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/tvFood"/>
            <TextView
               android:id="@+id/tvWater"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginLeft="3dp"
               android:layout_marginRight="10dp"
               android:text="물"
               android:textSize="12dp"
               android:textColor="@color/black"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/ivWater"/>
            <ImageView
               android:id="@+id/ivExercise"
               android:layout_width="6dp"
               android:layout_height="6dp"
               android:src="@drawable/oval"
               android:layout_marginLeft="7dp"
               app:tint="#FDA009"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/tvWater"/>
            <TextView
               android:id="@+id/tvExercise"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginLeft="3dp"
               android:layout_marginRight="10dp"
               android:text="운동"
               android:textSize="12dp"
               android:textColor="@color/black"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/ivExercise"/>
            <ImageView
               android:id="@+id/ivBody"
               android:layout_width="6dp"
               android:layout_height="6dp"
               android:src="@drawable/oval"
               android:layout_marginLeft="7dp"
               app:tint="#7FB540"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/tvExercise"/>
            <TextView
               android:id="@+id/tvBody"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginLeft="3dp"
               android:layout_marginRight="10dp"
               android:text="신체"
               android:textSize="12dp"
               android:textColor="@color/black"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/ivBody"/>
            <ImageView
               android:id="@+id/ivSleep"
               android:layout_width="6dp"
               android:layout_height="6dp"
               android:src="@drawable/oval"
               android:layout_marginLeft="7dp"
               app:tint="#4E698A"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/tvBody"/>
            <TextView
               android:id="@+id/tvSleep"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginLeft="3dp"
               android:layout_marginRight="10dp"
               android:text="수면"
               android:textSize="12dp"
               android:textColor="@color/black"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/ivSleep"/>
            <ImageView
               android:id="@+id/ivDrug"
               android:layout_width="6dp"
               android:layout_height="6dp"
               android:src="@drawable/oval"
               android:layout_marginLeft="7dp"
               app:tint="#A47AE8"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/tvSleep"/>
            <TextView
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginLeft="3dp"
               android:layout_marginRight="5dp"
               android:text="약복용"
               android:textColor="@color/black"
               android:textSize="12dp"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/ivDrug"/>
         </androidx.constraintlayout.widget.ConstraintLayout>
         <androidx.viewpager.widget.ViewPager
            android:id="@+id/viewPager"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_centerInParent="true"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="8dp"
            android:layout_marginRight="15dp"
            android:layout_marginBottom="15dp"
            android:clipToPadding="false"
            android:overScrollMode="never"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/cvList"/>
      </androidx.constraintlayout.widget.ConstraintLayout>
   </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>