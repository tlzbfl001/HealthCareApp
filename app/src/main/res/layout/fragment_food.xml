<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:app="http://schemas.android.com/apk/res-auto"
   android:layout_width="match_parent"
   android:layout_height="match_parent"
   android:background="@color/white_black">
   <androidx.core.widget.NestedScrollView
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:fillViewport="true"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintBottom_toBottomOf="parent">
      <LinearLayout
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:orientation="vertical">
         <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">
            <androidx.constraintlayout.widget.ConstraintLayout
               android:id="@+id/clGoal"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginRight="25dp"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintEnd_toEndOf="parent">
               <ImageView
                  android:layout_width="16dp"
                  android:layout_height="21dp"
                  android:src="@drawable/setting_goal"
                  android:layout_marginTop="1dp"
                  app:tint="@color/black_white"
                  app:layout_constraintTop_toTopOf="@id/tvSetting"
                  app:layout_constraintBottom_toBottomOf="@id/tvSetting"
                  app:layout_constraintEnd_toStartOf="@id/tvSetting"/>
               <TextView
                  android:id="@+id/tvSetting"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:paddingLeft="3dp"
                  android:text="목표설정"
                  android:textColor="@color/black_white"
                  android:textSize="15dp"
                  app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintEnd_toEndOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
               android:id="@+id/cl2"
               android:layout_width="180dp"
               android:layout_height="180dp"
               android:layout_marginTop="18dp"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintEnd_toEndOf="parent"
               app:layout_constraintStart_toStartOf="parent">
               <com.dinuscxj.progressbar.CircleProgressBar
                  android:id="@+id/pbFood"
                  android:layout_width="match_parent"
                  android:layout_height="match_parent"
                  app:progress_text_size="0dp"
                  app:progress_background_color="@color/progress_base"
                  app:progress_end_color="@color/food"
                  app:progress_start_color="@color/food"
                  app:progress_start_degree="90"
                  app:progress_stroke_cap="round"
                  app:progress_stroke_width="14.5dp"
                  app:progress_style="solid_line" />
               <androidx.constraintlayout.widget.ConstraintLayout
                  android:layout_width="match_parent"
                  android:layout_height="match_parent"
                  android:layout_margin="18dp"
                  android:background="@drawable/oval">
                  <TextView
                     android:id="@+id/tvFood1"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:text="섭취칼로리"
                     android:textColor="@color/black_white"
                     android:textSize="16.5dp"
                     android:textStyle="bold"
                     app:layout_constraintBottom_toTopOf="@id/tvIntake"
                     app:layout_constraintEnd_toEndOf="parent"
                     app:layout_constraintStart_toStartOf="parent"
                     app:layout_constraintTop_toTopOf="parent"
                     app:layout_constraintVertical_chainStyle="packed" />
                  <TextView
                     android:id="@+id/tvIntake"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:text="0 kcal"
                     android:textColor="@color/black_white"
                     android:textSize="16.5dp"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintEnd_toEndOf="parent"
                     app:layout_constraintStart_toStartOf="parent"
                     app:layout_constraintTop_toBottomOf="@id/tvFood1" />
               </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <TextView
               android:id="@+id/tv1"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginTop="20dp"
               android:text="목표칼로리"
               android:textSize="16.5dp"
               android:textStyle="bold"
               android:textColor="@color/black_white"
               app:layout_constraintTop_toBottomOf="@id/cl2"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintEnd_toStartOf="@id/view1"
               app:layout_constraintHorizontal_chainStyle="packed"/>
            <View
               android:id="@+id/view1"
               android:layout_width="1dp"
               android:layout_height="40dp"
               android:background="@color/black_white"
               android:layout_marginLeft="15dp"
               app:layout_constraintStart_toEndOf="@id/tv1"
               app:layout_constraintTop_toTopOf="@id/tv1"
               app:layout_constraintEnd_toStartOf="@id/tv3"
               app:layout_constraintBottom_toBottomOf="@id/tvGoal"/>
            <TextView
               android:id="@+id/tvGoal"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:text="0 kcal"
               android:textSize="16.5dp"
               android:textColor="@color/black_white"
               app:layout_constraintTop_toBottomOf="@id/tv1"
               app:layout_constraintEnd_toEndOf="@id/tv1"/>
            <TextView
               android:id="@+id/tv3"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginLeft="15dp"
               android:text="남은칼로리"
               android:textSize="16.5dp"
               android:textStyle="bold"
               android:textColor="@color/black_white"
               app:layout_constraintTop_toTopOf="@id/tv1"
               app:layout_constraintEnd_toEndOf="parent"
               app:layout_constraintStart_toEndOf="@id/view1"/>
            <TextView
               android:id="@+id/tvRemain"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:text="0 kcal"
               android:textSize="16.5dp"
               android:textColor="@color/black_white"
               app:layout_constraintTop_toBottomOf="@id/tv3"
               app:layout_constraintStart_toStartOf="@id/tv3"/>
         </androidx.constraintlayout.widget.ConstraintLayout>
         <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/rec_top_radius_35"
            android:backgroundTint="@color/food_grey"
            android:layout_marginTop="20dp">
            <TextView
               android:id="@+id/tv4"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginTop="25dp"
               android:layout_marginLeft="25dp"
               android:text="하루 식단"
               android:textColor="@color/black_white"
               android:textSize="16.5dp"
               android:textStyle="bold"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintStart_toStartOf="parent"/>
            <androidx.constraintlayout.widget.ConstraintLayout
               android:id="@+id/clRecord"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               app:layout_constraintTop_toTopOf="@id/tv4"
               app:layout_constraintBottom_toBottomOf="@id/tv4"
               app:layout_constraintEnd_toEndOf="parent">
               <ImageView
                  android:id="@+id/iv2"
                  android:layout_width="16dp"
                  android:layout_height="21dp"
                  android:src="@drawable/setting_goal"
                  android:layout_marginTop="2dp"
                  android:layout_marginRight="3dp"
                  app:layout_constraintTop_toTopOf="@id/tv5"
                  app:layout_constraintBottom_toBottomOf="@id/tv5"
                  app:layout_constraintEnd_toStartOf="@id/tv5"
                  app:tint="@color/black_white" />
               <TextView
                  android:id="@+id/tv5"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_marginRight="25dp"
                  android:text="섭취기록"
                  android:textColor="@color/black_white"
                  android:textSize="15dp"
                  app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintBottom_toBottomOf="parent"
                  app:layout_constraintEnd_toEndOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.cardview.widget.CardView
               android:id="@+id/cv1"
               android:layout_width="match_parent"
               android:layout_height="220dp"
               android:layout_marginTop="15dp"
               android:layout_marginLeft="25dp"
               android:layout_marginRight="25dp"
               app:cardCornerRadius="15dp"
               app:cardElevation="0dp"
               app:layout_constraintTop_toBottomOf="@id/tv4"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintBottom_toTopOf="@id/cv2"
               app:layout_constraintEnd_toEndOf="parent">
               <androidx.constraintlayout.widget.ConstraintLayout
                  android:layout_width="match_parent"
                  android:layout_height="match_parent">
                  <androidx.viewpager.widget.ViewPager
                     android:id="@+id/viewPager"
                     android:layout_width="match_parent"
                     android:layout_height="match_parent"
                     android:background="@color/light_grey_grey"
                     app:layout_constraintTop_toTopOf="parent"
                     app:layout_constraintStart_toStartOf="parent"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintEnd_toEndOf="parent"/>
                  <androidx.constraintlayout.widget.ConstraintLayout
                     android:id="@+id/clLeft"
                     android:layout_width="wrap_content"
                     android:layout_height="match_parent"
                     app:layout_constraintTop_toTopOf="parent"
                     app:layout_constraintStart_toStartOf="parent"
                     app:layout_constraintBottom_toBottomOf="parent">
                     <ImageView
                        android:layout_width="11dp"
                        android:layout_height="wrap_content"
                        android:src="@drawable/arrow_left"
                        app:tint="@color/white"
                        android:layout_marginLeft="8dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"/>
                  </androidx.constraintlayout.widget.ConstraintLayout>
                  <androidx.constraintlayout.widget.ConstraintLayout
                     android:id="@+id/clRight"
                     android:layout_width="wrap_content"
                     android:layout_height="match_parent"
                     app:layout_constraintTop_toTopOf="parent"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintEnd_toEndOf="parent">
                     <ImageView
                        android:layout_width="11dp"
                        android:layout_height="wrap_content"
                        android:src="@drawable/arrow_right"
                        app:tint="@color/white"
                        android:layout_marginRight="8dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"/>
                  </androidx.constraintlayout.widget.ConstraintLayout>
               </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
               android:id="@+id/cv2"
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:layout_marginTop="18dp"
               android:layout_marginLeft="25dp"
               android:layout_marginRight="25dp"
               app:cardBackgroundColor="@color/food"
               app:cardCornerRadius="30dp"
               app:cardElevation="0dp"
               app:layout_constraintTop_toBottomOf="@id/cv1"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintEnd_toEndOf="parent">
               <androidx.constraintlayout.widget.ConstraintLayout
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:paddingTop="7dp"
                  android:paddingBottom="7dp">
                  <TextView
                     android:id="@+id/tvCalPct"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:layout_marginLeft="20dp"
                     android:text="순탄수 0%"
                     android:textSize="14dp"
                     android:textColor="@color/white"
                     android:textAlignment="center"
                     android:maxLines="1"
                     android:ellipsize="end"
                     app:layout_constraintTop_toTopOf="parent"
                     app:layout_constraintEnd_toStartOf="@id/tvProteinPct"
                     app:layout_constraintStart_toStartOf="parent"/>
                  <TextView
                     android:id="@+id/tvCar"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:text="0 g"
                     android:textSize="16dp"
                     android:textStyle="bold"
                     android:textColor="@color/white"
                     android:textAlignment="center"
                     android:maxLines="1"
                     android:ellipsize="end"
                     app:layout_constraintTop_toBottomOf="@id/tvCalPct"
                     app:layout_constraintStart_toStartOf="@id/tvCalPct"
                     app:layout_constraintEnd_toEndOf="@id/tvCalPct"
                     app:layout_constraintBottom_toBottomOf="parent"/>
                  <TextView
                     android:id="@+id/tvProteinPct"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:layout_marginLeft="20dp"
                     android:text="단백질 0%"
                     android:textSize="14dp"
                     android:textColor="@color/white"
                     android:textAlignment="center"
                     android:maxLines="1"
                     android:ellipsize="end"
                     app:layout_constraintStart_toEndOf="@id/tvCalPct"
                     app:layout_constraintEnd_toStartOf="@id/tvFatPct"
                     app:layout_constraintTop_toTopOf="parent"/>
                  <TextView
                     android:id="@+id/tvProtein"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:text="0 g"
                     android:textSize="16dp"
                     android:textStyle="bold"
                     android:textColor="@color/white"
                     android:textAlignment="center"
                     android:maxLines="1"
                     android:ellipsize="end"
                     app:layout_constraintTop_toBottomOf="@id/tvProteinPct"
                     app:layout_constraintStart_toStartOf="@id/tvProteinPct"
                     app:layout_constraintEnd_toEndOf="@id/tvProteinPct"
                     app:layout_constraintBottom_toBottomOf="parent"/>
                  <TextView
                     android:id="@+id/tvFatPct"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:layout_marginLeft="20dp"
                     android:layout_marginRight="20dp"
                     android:text="지방0%"
                     android:textSize="14dp"
                     android:textColor="@color/white"
                     android:textAlignment="center"
                     android:maxLines="1"
                     android:ellipsize="end"
                     app:layout_constraintStart_toEndOf="@id/tvProteinPct"
                     app:layout_constraintTop_toTopOf="parent"
                     app:layout_constraintEnd_toEndOf="parent"/>
                  <TextView
                     android:id="@+id/tvFat"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:text="0 g"
                     android:textSize="16dp"
                     android:textStyle="bold"
                     android:textColor="@color/white"
                     android:textAlignment="center"
                     android:maxLines="1"
                     android:ellipsize="end"
                     app:layout_constraintStart_toStartOf="@id/tvFatPct"
                     app:layout_constraintTop_toBottomOf="@id/tvFatPct"
                     app:layout_constraintEnd_toEndOf="@id/tvFatPct"
                     app:layout_constraintBottom_toBottomOf="parent"/>
               </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
               android:id="@+id/cvBreakfast"
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:layout_marginTop="16dp"
               android:layout_marginLeft="25dp"
               android:layout_marginRight="25dp"
               app:cardBackgroundColor="@color/white_grey"
               app:cardElevation="3dp"
               app:cardCornerRadius="15dp"
               app:layout_constraintTop_toBottomOf="@id/cv2"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintEnd_toEndOf="parent">
               <androidx.constraintlayout.widget.ConstraintLayout
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content">
                  <TextView
                     android:id="@+id/tvBreakfast"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_marginLeft="15dp"
                     android:text="아침"
                     android:textSize="16.5dp"
                     android:textStyle="bold"
                     android:textColor="@color/black_white"
                     app:layout_constraintTop_toTopOf="@id/clExpand1"
                     app:layout_constraintBottom_toBottomOf="@id/clExpand1"
                     app:layout_constraintStart_toStartOf="parent"/>
                  <TextView
                     android:id="@+id/tvTotal1"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_marginLeft="20dp"
                     android:text="0 kcal"
                     android:textSize="16.5dp"
                     android:textColor="@color/black_white"
                     app:layout_constraintStart_toEndOf="@id/tvBreakfast"
                     app:layout_constraintTop_toTopOf="@id/tvBreakfast"
                     app:layout_constraintBottom_toBottomOf="@id/tvBreakfast"/>
                  <androidx.constraintlayout.widget.ConstraintLayout
                     android:id="@+id/clExpand1"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     app:layout_constraintTop_toTopOf="parent"
                     app:layout_constraintBottom_toTopOf="@id/view"
                     app:layout_constraintEnd_toEndOf="parent">
                     <ImageView
                        android:id="@+id/ivExpand1"
                        android:layout_width="20dp"
                        android:layout_height="10dp"
                        android:src="@drawable/arrow_down"
                        android:layout_marginTop="25dp"
                        android:layout_marginBottom="25dp"
                        android:layout_marginRight="15dp"
                        app:tint="@color/black_white"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent" />
                  </androidx.constraintlayout.widget.ConstraintLayout>
                  <View
                     android:id="@+id/view"
                     android:layout_width="match_parent"
                     android:layout_height="0dp"
                     app:layout_constraintTop_toBottomOf="@id/clExpand1"
                     app:layout_constraintStart_toStartOf="parent"
                     app:layout_constraintEnd_toEndOf="parent"/>
                  <androidx.constraintlayout.widget.ConstraintLayout
                     android:id="@+id/clView1"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:visibility="gone"
                     app:layout_constraintTop_toBottomOf="@id/view"
                     app:layout_constraintStart_toStartOf="parent"
                     app:layout_constraintEnd_toEndOf="parent"
                     app:layout_constraintBottom_toBottomOf="parent">
                     <View
                        android:id="@+id/view2"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#ddd"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"/>
                     <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/rv1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:layout_marginBottom="7dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        app:layout_constraintTop_toTopOf="@id/view2"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"/>
                  </androidx.constraintlayout.widget.ConstraintLayout>
               </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
               android:id="@+id/cvLunch"
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:layout_marginTop="16dp"
               android:layout_marginLeft="25dp"
               android:layout_marginRight="25dp"
               app:cardBackgroundColor="@color/white_grey"
               app:cardElevation="3dp"
               app:cardCornerRadius="15dp"
               app:layout_constraintTop_toBottomOf="@id/cvBreakfast"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintEnd_toEndOf="parent">
               <androidx.constraintlayout.widget.ConstraintLayout
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content">
                  <TextView
                     android:id="@+id/tvLunch"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_marginLeft="15dp"
                     android:text="점심"
                     android:textSize="16.5dp"
                     android:textStyle="bold"
                     android:textColor="@color/black_white"
                     app:layout_constraintTop_toTopOf="@id/clExpand2"
                     app:layout_constraintBottom_toBottomOf="@id/clExpand2"
                     app:layout_constraintStart_toStartOf="parent"/>
                  <TextView
                     android:id="@+id/tvTotal2"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_marginLeft="20dp"
                     android:text="0 kcal"
                     android:textSize="16.5dp"
                     android:textColor="@color/black_white"
                     app:layout_constraintStart_toEndOf="@id/tvLunch"
                     app:layout_constraintTop_toTopOf="@id/tvLunch"
                     app:layout_constraintBottom_toBottomOf="@id/tvLunch"/>
                  <androidx.constraintlayout.widget.ConstraintLayout
                     android:id="@+id/clExpand2"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     app:layout_constraintTop_toTopOf="parent"
                     app:layout_constraintBottom_toTopOf="@id/view3"
                     app:layout_constraintEnd_toEndOf="parent">
                     <ImageView
                        android:id="@+id/ivExpand2"
                        android:layout_width="20dp"
                        android:layout_height="10dp"
                        android:src="@drawable/arrow_down"
                        android:layout_marginTop="25dp"
                        android:layout_marginBottom="25dp"
                        android:layout_marginRight="15dp"
                        app:tint="@color/black_white"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"/>
                  </androidx.constraintlayout.widget.ConstraintLayout>
                  <View
                     android:id="@+id/view3"
                     android:layout_width="match_parent"
                     android:layout_height="0dp"
                     app:layout_constraintTop_toBottomOf="@id/clExpand2"
                     app:layout_constraintStart_toStartOf="parent"
                     app:layout_constraintEnd_toEndOf="parent"/>
                  <androidx.constraintlayout.widget.ConstraintLayout
                     android:id="@+id/clView2"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:visibility="gone"
                     app:layout_constraintTop_toBottomOf="@id/view3"
                     app:layout_constraintStart_toStartOf="parent"
                     app:layout_constraintEnd_toEndOf="parent"
                     app:layout_constraintBottom_toBottomOf="parent">
                     <View
                        android:id="@+id/view4"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#ddd"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"/>
                     <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/rv2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:layout_marginBottom="7dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        app:layout_constraintTop_toTopOf="@id/view4"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"/>
                  </androidx.constraintlayout.widget.ConstraintLayout>
               </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
               android:id="@+id/cvDinner"
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:layout_marginTop="16dp"
               android:layout_marginLeft="25dp"
               android:layout_marginRight="25dp"
               app:cardBackgroundColor="@color/white_grey"
               app:cardElevation="3dp"
               app:cardCornerRadius="15dp"
               app:layout_constraintTop_toBottomOf="@id/cvLunch"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintEnd_toEndOf="parent">
               <androidx.constraintlayout.widget.ConstraintLayout
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content">
                  <TextView
                     android:id="@+id/tvDinner"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_marginLeft="15dp"
                     android:text="저녁"
                     android:textSize="16.5dp"
                     android:textStyle="bold"
