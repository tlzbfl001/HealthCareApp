<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:app="http://schemas.android.com/apk/res-auto"
   android:layout_width="match_parent"
   android:layout_height="match_parent"
   android:orientation="vertical"
   android:background="@color/white">
   <ScrollView
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:fillViewport="true"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintBottom_toBottomOf="parent">
      <androidx.constraintlayout.widget.ConstraintLayout
         android:layout_width="match_parent"
         android:layout_height="wrap_content">
         <ImageView
            android:layout_width="145dp"
            android:layout_height="145dp"
            android:background="@drawable/food_top"
            android:alpha="0.9"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
         <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clBack"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="30dp"
            android:paddingLeft="25dp"
            android:paddingBottom="10dp"
            android:paddingRight="10dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent">
            <ImageView
                android:layout_width="15dp"
                android:layout_height="25dp"
                android:src="@drawable/arrow_left"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"/>
         </androidx.constraintlayout.widget.ConstraintLayout>
         <ImageView
            android:id="@+id/ivLogo"
            android:layout_width="120dp"
            android:layout_height="13dp"
            android:src="@drawable/logo3"
            android:layout_marginTop="30dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
         <androidx.cardview.widget.CardView
            android:id="@+id/cvDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="17dp"
            app:cardBackgroundColor="#FBFAFA"
            app:cardCornerRadius="15dp"
            app:cardElevation="0dp"
            app:layout_constraintTop_toBottomOf="@id/ivLogo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">
            <androidx.constraintlayout.widget.ConstraintLayout
               android:layout_width="wrap_content"
               android:layout_height="wrap_content">
               <TextView
                  android:id="@+id/tvPrev"
                  android:layout_width="25dp"
                  android:layout_height="24dp"
                  android:background="@drawable/ic_left_triangle"
                  android:layout_marginLeft="7dp"
                  android:layout_marginRight="7dp"
                  app:layout_constraintStart_toStartOf="parent"
                  app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintBottom_toBottomOf="parent"
                  app:layout_constraintEnd_toStartOf="@id/tvDate"/>
               <TextView
                  android:id="@+id/tvDate"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="2023년 7월 23일"
                  android:textColor="@color/black"
                  android:textSize="15dp"
                  app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintBottom_toBottomOf="parent"
                  app:layout_constraintEnd_toStartOf="@id/tvNext"
                  app:layout_constraintStart_toEndOf="@id/tvPrev"/>
               <TextView
                  android:id="@+id/tvNext"
                  android:layout_width="25dp"
                  android:layout_height="24dp"
                  android:background="@drawable/ic_right_triangle"
                  android:layout_marginLeft="7dp"
                  android:layout_marginRight="7dp"
                  app:layout_constraintStart_toEndOf="@id/tvDate"
                  app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintBottom_toBottomOf="parent"
                  app:layout_constraintEnd_toEndOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
         </androidx.cardview.widget.CardView>
         <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/cvDate"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">
            <androidx.cardview.widget.CardView
               android:id="@+id/cvFood"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:layout_marginTop="17dp"
               android:layout_marginBottom="17dp"
               android:layout_marginRight="8dp"
               android:layout_marginLeft="25dp"
               app:cardBackgroundColor="#EE6685"
               app:cardCornerRadius="6dp"
               app:cardElevation="3dp"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintEnd_toStartOf="@id/cvWater"
               app:layout_constraintHorizontal_weight="3">
               <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_gravity="center"
                  android:paddingTop="6dp"
                  android:paddingBottom="6dp"
                  android:text="식단"
                  android:textSize="18dp"
                  android:textColor="@color/white"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
               android:id="@+id/cvWater"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:layout_marginTop="17dp"
               android:layout_marginBottom="17dp"
               android:layout_marginRight="8dp"
               app:cardBackgroundColor="@color/white"
               app:cardCornerRadius="6dp"
               app:cardElevation="3dp"
               app:layout_constraintStart_toEndOf="@id/cvFood"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintEnd_toStartOf="@id/cvExercise"
               app:layout_constraintHorizontal_weight="2">
               <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_gravity="center"
                  android:paddingTop="6dp"
                  android:paddingBottom="6dp"
                  android:text="물"
                  android:textSize="18dp"
                  android:textColor="#888888"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
               android:id="@+id/cvExercise"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:layout_marginTop="17dp"
               android:layout_marginBottom="17dp"
               android:layout_marginRight="8dp"
               app:cardBackgroundColor="@color/white"
               app:cardCornerRadius="6dp"
               app:cardElevation="3dp"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/cvWater"
               app:layout_constraintEnd_toStartOf="@id/cvBody"
               app:layout_constraintHorizontal_weight="3">
               <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_gravity="center"
                  android:paddingTop="6dp"
                  android:paddingBottom="6dp"
                  android:text="운동"
                  android:textSize="18dp"
                  android:textColor="#888888"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
               android:id="@+id/cvBody"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:layout_marginTop="17dp"
               android:layout_marginBottom="17dp"
               android:layout_marginRight="8dp"
               app:cardBackgroundColor="@color/white"
               app:cardCornerRadius="6dp"
               app:cardElevation="3dp"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/cvExercise"
               app:layout_constraintEnd_toStartOf="@id/cvSleep"
               app:layout_constraintHorizontal_weight="3">
               <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_gravity="center"
                  android:paddingTop="6dp"
                  android:paddingBottom="6dp"
                  android:text="신체"
                  android:textSize="18dp"
                  android:textColor="#888888"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
               android:id="@+id/cvSleep"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:layout_marginTop="17dp"
               android:layout_marginBottom="17dp"
               android:layout_marginRight="8dp"
               app:cardBackgroundColor="@color/white"
               app:cardCornerRadius="6dp"
               app:cardElevation="3dp"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/cvBody"
               app:layout_constraintEnd_toStartOf="@id/cvDrug"
               app:layout_constraintHorizontal_weight="3">
               <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_gravity="center"
                  android:paddingTop="6dp"
                  android:paddingBottom="6dp"
                  android:text="수면"
                  android:textSize="18dp"
                  android:textColor="#888888"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
               android:id="@+id/cvDrug"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:layout_marginTop="17dp"
               android:layout_marginBottom="17dp"
               android:layout_marginRight="25dp"
               app:cardBackgroundColor="@color/white"
               app:cardCornerRadius="6dp"
               app:cardElevation="3dp"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintStart_toEndOf="@id/cvSleep"
               app:layout_constraintEnd_toEndOf="parent"
               app:layout_constraintHorizontal_weight="4">
               <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_gravity="center"
                  android:paddingTop="6dp"
                  android:paddingBottom="6dp"
                  android:text="약복용"
                  android:textSize="18dp"
                  android:textColor="#888888"/>
            </androidx.cardview.widget.CardView>
         </androidx.constraintlayout.widget.ConstraintLayout>
         <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/cl1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@id/cl3">
            <androidx.constraintlayout.widget.ConstraintLayout
               android:id="@+id/clLeft"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:layout_marginLeft="25dp"
               android:background="@drawable/rec_detail"
               android:paddingTop="25dp"
               android:paddingBottom="25dp"
               app:layout_constraintEnd_toStartOf="@id/clRight"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintHorizontal_weight="5">
               <androidx.constraintlayout.widget.ConstraintLayout
                  android:layout_width="120dp"
                  android:layout_height="120dp"
                  app:layout_constraintBottom_toBottomOf="parent"
                  app:layout_constraintEnd_toEndOf="parent"
                  app:layout_constraintStart_toStartOf="parent"
                  app:layout_constraintTop_toTopOf="parent">
                  <com.dinuscxj.progressbar.CircleProgressBar
                     android:id="@+id/pbFood"
                     android:layout_width="match_parent"
                     android:layout_height="match_parent"
                     app:progress_background_color="#33DF9EAD"
                     app:progress_end_color="#EE6685"
                     app:progress_start_color="#EE6685"
                     app:progress_start_degree="90"
                     app:progress_stroke_cap="round"
                     app:progress_stroke_width="12dp"
                     app:progress_style="solid_line" />
                  <androidx.constraintlayout.widget.ConstraintLayout
                     android:layout_width="match_parent"
                     android:layout_height="match_parent"
                     android:layout_margin="18dp"
                     android:background="@drawable/oval_border_food">
                     <TextView
                        android:id="@+id/tvFood1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="섭취칼로리"
                        android:textColor="@color/black"
                        android:textSize="12dp"
                        app:layout_constraintBottom_toTopOf="@id/tvIntake"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_chainStyle="packed" />
                     <TextView
                        android:id="@+id/tvIntake"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0 kcal"
                        android:textColor="@color/black"
                        android:textSize="12dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tvFood1" />
                  </androidx.constraintlayout.widget.ConstraintLayout>
               </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
               android:id="@+id/clRight"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:layout_marginRight="25dp"
               app:layout_constraintTop_toTopOf="@id/clLeft"
               app:layout_constraintEnd_toEndOf="parent"
               app:layout_constraintStart_toEndOf="@id/clLeft"
               app:layout_constraintHorizontal_weight="3" >
               <androidx.cardview.widget.CardView
                  android:id="@+id/cvGoal"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_marginTop="5dp"
                  android:layout_marginRight="5dp"
                  app:cardBackgroundColor="#F2F2F2"
                  app:cardCornerRadius="8dp"
                  app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintEnd_toEndOf="parent">
                  <androidx.constraintlayout.widget.ConstraintLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:paddingTop="5dp"
                     android:paddingBottom="5dp"
                     android:paddingLeft="10dp"
                     android:paddingRight="10dp">
                     <ImageView
                        android:id="@+id/iv1"
                        android:layout_width="11dp"
                        android:layout_height="14dp"
                        android:src="@drawable/setting_goal"
                        app:tint="@color/black"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"/>
                     <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:paddingLeft="5dp"
                        android:text="목표설정"
                        android:textColor="@color/black"
                        android:textSize="14dp"
                        app:layout_constraintStart_toEndOf="@id/iv1"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"/>
                  </androidx.constraintlayout.widget.ConstraintLayout>
               </androidx.cardview.widget.CardView>
               <TextView
                  android:id="@+id/tv2"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_marginTop="15dp"
                  android:text="목표칼로리"
                  android:textColor="#999999"
                  android:textSize="12dp"
                  app:layout_constraintTop_toBottomOf="@id/cvGoal"
                  app:layout_constraintEnd_toEndOf="@id/cvGoal"/>
               <TextView
                  android:id="@+id/tvGoal"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="0 kcal"
                  android:textColor="@color/black"
                  android:textSize="18dp"
                  app:layout_constraintTop_toBottomOf="@id/tv2"
                  app:layout_constraintEnd_toEndOf="@id/cvGoal"/>
               <TextView
                  android:id="@+id/tv4"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_marginTop="10dp"
                  android:text="남은칼로리"
                  android:textColor="#999999"
                  android:textSize="12dp"
                  app:layout_constraintTop_toBottomOf="@id/tvGoal"
                  app:layout_constraintEnd_toEndOf="@id/cvGoal" />
               <TextView
                  android:id="@+id/tvRemain"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="0 kcal"
                  android:textColor="@color/black"
                  android:textSize="18dp"
                   app:layout_constraintTop_toBottomOf="@id/tv4"
                   app:layout_constraintEnd_toEndOf="@id/cvGoal" />
            </androidx.constraintlayout.widget.ConstraintLayout>
         </androidx.constraintlayout.widget.ConstraintLayout>
         <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl3"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/cl2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_weight="1">
            <TextView
               android:id="@+id/tv6"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginLeft="25dp"
               android:text="하루 식단"
               android:textColor="#777777"
               android:textSize="16dp"
               app:layout_constraintTop_toTopOf="@id/clRecord"
               app:layout_constraintBottom_toBottomOf="@id/clRecord"
               app:layout_constraintStart_toStartOf="parent"/>
            <androidx.constraintlayout.widget.ConstraintLayout
               android:id="@+id/clRecord"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:background="@drawable/detail_bar"
               android:elevation="5dp"
               android:layout_marginTop="20dp"
               android:paddingLeft="7dp"
               android:paddingTop="5dp"
               android:paddingBottom="5dp"
               android:paddingRight="27dp"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintEnd_toEndOf="parent">
               <ImageView
                  android:id="@+id/iv2"
                  android:layout_width="18dp"
                  android:layout_height="18dp"
                  android:background="@drawable/oval"
                  android:src="@drawable/pencil"
                  android:padding="3.3dp"
                  app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintStart_toStartOf="parent"
                  app:layout_constraintBottom_toBottomOf="parent" />
               <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_marginLeft="5dp"
                  android:text="섭취 식단 기록"
                  android:textColor="@color/white"
                  android:textSize="14dp"
                  app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintBottom_toBottomOf="parent"
                  app:layout_constraintStart_toEndOf="@id/iv2"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.cardview.widget.CardView
               android:layout_width="match_parent"
               android:layout_height="0dp"
               android:layout_marginTop="15dp"
               android:layout_marginLeft="25dp"
               android:layout_marginRight="25dp"
               android:layout_marginBottom="30dp"
               app:cardBackgroundColor="@color/white"
               app:cardCornerRadius="10dp"
               app:cardElevation="8dp"
               app:layout_constraintTop_toBottomOf="@id/clRecord"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintEnd_toEndOf="parent"
               app:layout_constraintBottom_toBottomOf="parent">
               <androidx.constraintlayout.widget.ConstraintLayout
                  android:layout_width="match_parent"
                  android:layout_height="match_parent">
                  <androidx.constraintlayout.widget.ConstraintLayout
                     android:id="@+id/clBox1"
                     android:layout_width="wrap_content"
                     android:layout_height="match_parent"
                     app:layout_constraintTop_toTopOf="parent"
                     app:layout_constraintStart_toStartOf="parent"
                     app:layout_constraintBottom_toBottomOf="parent">
                     <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/clBtn1"
                        android:layout_width="80dp"
                        android:layout_height="0dp"
                        android:layout_marginRight="15dp"
                        android:background="@drawable/food_btn1"
                        android:elevation="5dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toTopOf="@id/clBtn2"
                        app:layout_constraintVertical_weight="1">
                        <TextView
                           android:id="@+id/tvBtn1Title"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:text="아침"
                           android:textSize="16dp"
                           android:textColor="@color/white"
                           app:layout_constraintTop_toTopOf="parent"
                           app:layout_constraintStart_toStartOf="parent"
                           app:layout_constraintBottom_toTopOf="@id/tvBtn1Desc"
                           app:layout_constraintEnd_toEndOf="parent"
                           app:layout_constraintVertical_chainStyle="packed"/>
                        <TextView
                           android:id="@+id/tvBtn1Desc"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:text="0 kcal"
                           android:textSize="12dp"
                           android:textColor="@color/white"
                           app:layout_constraintTop_toBottomOf="@id/tvBtn1Title"
                           app:layout_constraintStart_toStartOf="parent"
                           app:layout_constraintEnd_toEndOf="parent"
                           app:layout_constraintBottom_toBottomOf="parent"/>
                     </androidx.constraintlayout.widget.ConstraintLayout>
                     <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/clBtn2"
                        android:layout_width="80dp"
                        android:layout_height="0dp"
                        android:layout_marginRight="15dp"
                        android:background="@drawable/food_btn2"
                        android:elevation="5dp"
                        app:layout_constraintTop_toBottomOf="@id/clBtn1"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toTopOf="@id/clBtn3"
                        app:layout_constraintVertical_weight="1">
                        <TextView
                           android:id="@+id/tvBtn2Title"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:layout_marginTop="4dp"
                           android:text="점심"
                           android:textSize="16dp"
                           android:textColor="@color/black"
                           app:layout_constraintTop_toTopOf="parent"
                           app:layout_constraintStart_toStartOf="parent"
                           app:layout_constraintEnd_toEndOf="parent"
                           app:layout_constraintBottom_toTopOf="@id/tvBtn2Desc"
                           app:layout_constraintVertical_chainStyle="packed"/>
                        <TextView
                           android:id="@+id/tvBtn2Desc"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:text="0 kcal"
                           android:textSize="12dp"
                           android:textColor="@color/black"
                           app:layout_constraintTop_toBottomOf="@id/tvBtn2Title"
                           app:layout_constraintStart_toStartOf="parent"
                           app:layout_constraintEnd_toEndOf="parent"
                           app:layout_constraintBottom_toBottomOf="parent"/>
                     </androidx.constraintlayout.widget.ConstraintLayout>
                     <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/clBtn3"
                        android:layout_width="80dp"
                        android:layout_height="0dp"
                        android:layout_marginRight="15dp"
                        android:background="@drawable/food_btn2"
                        android:backgroundTint="#FFD9D9"
                        android:elevation="5dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/clBtn2"
                        app:layout_constraintBottom_toTopOf="@id/clBtn4"
                        app:layout_constraintVertical_weight="1">
                        <TextView
                           android:id="@+id/tvBtn3Title"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:layout_marginTop="4dp"
                           android:text="저녁"
                           android:textSize="16dp"
                           android:textColor="@color/black"
                           app:layout_constraintTop_toTopOf="parent"
                           app:layout_constraintStart_toStartOf="parent"
                           app:layout_constraintEnd_toEndOf="parent"
                           app:layout_constraintBottom_toTopOf="@id/tvBtn3Desc"
                           app:layout_constraintVertical_chainStyle="packed"/>
                        <TextView
                           android:id="@+id/tvBtn3Desc"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:text="0 kcal"
                           android:textSize="12dp"
                           android:textColor="@color/black"
                           app:layout_constraintTop_toBottomOf="@id/tvBtn3Title"
                           app:layout_constraintStart_toStartOf="parent"
                           app:layout_constraintEnd_toEndOf="parent"
                           app:layout_constraintBottom_toBottomOf="parent"/>
                     </androidx.constraintlayout.widget.ConstraintLayout>
                     <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/clBtn4"
                        android:layout_width="80dp"
                        android:layout_height="0dp"
                        android:layout_marginRight="15dp"
                        android:background="@drawable/food_btn2"
                        android:backgroundTint="#FFE9E9"
                        android:elevation="5dp"
                        app:layout_constraintTop_toBottomOf="@id/clBtn3"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintVertical_weight="1">
                        <TextView
                           android:id="@+id/tvBtn4Title"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:layout_marginTop="4dp"
                           android:text="간식"
                           android:textSize="16dp"
                           android:textColor="@color/black"
                           app:layout_constraintTop_toTopOf="parent"
                           app:layout_constraintStart_toStartOf="parent"
                           app:layout_constraintEnd_toEndOf="parent"
                           app:layout_constraintBottom_toTopOf="@id/tvBtn4Desc"
                           app:layout_constraintVertical_chainStyle="packed"/>
                        <TextView
                           android:id="@+id/tvBtn4Desc"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:text="0 kcal"
                           android:textSize="12dp"
                           android:textColor="@color/black"
                           app:layout_constraintTop_toBottomOf="@id/tvBtn4Title"
                           app:layout_constraintStart_toStartOf="parent"
                           app:layout_constraintEnd_toEndOf="parent"
                           app:layout_constraintBottom_toBottomOf="parent"/>
                     </androidx.constraintlayout.widget.ConstraintLayout>
                  </androidx.constraintlayout.widget.ConstraintLayout>
                  <androidx.constraintlayout.widget.ConstraintLayout
                     android:id="@+id/cl4"
                     android:layout_width="0dp"
                     android:layout_height="0dp"
                     app:layout_constraintStart_toEndOf="@id/clBox1"
                     app:layout_constraintTop_toTopOf="parent"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintEnd_toEndOf="parent"
                     app:layout_constraintHorizontal_weight="1"
                     app:layout_constraintVertical_weight="1">
                     <androidx.cardview.widget.CardView
                        android:id="@+id/cvSlide"
                        android:layout_width="match_parent"
                        android:layout_height="120dp"
                        android:layout_marginTop="12dp"
                        android:layout_marginRight="15dp"
                        app:cardCornerRadius="10dp"
                        app:cardElevation="0dp"
                        app:cardBackgroundColor="#F9F9F9"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toTopOf="@id/viewpager">
                        <androidx.constraintlayout.widget.ConstraintLayout
                           android:layout_width="match_parent"
                           android:layout_height="match_parent">
                           <androidx.constraintlayout.widget.ConstraintLayout
                              android:id="@+id/cvLeft"
                              android:layout_width="wrap_content"
                              android:layout_height="match_parent"
                              app:layout_constraintBottom_toBottomOf="parent"
                              app:layout_constraintEnd_toStartOf="@id/viewPager"
                              app:layout_constraintStart_toStartOf="parent"
                              app:layout_constraintTop_toTopOf="parent">
                              <ImageView
                                 android:layout_width="8dp"
                                 android:layout_height="wrap_content"
                                 android:src="@drawable/arrow_left_round"
                                 android:layout_marginLeft="7dp"
                                 android:layout_marginRight="3dp"
                                 app:layout_constraintBottom_toBottomOf="parent"
                                 app:layout_constraintStart_toStartOf="parent"
                                 app:layout_constraintTop_toTopOf="parent"
                                 app:layout_constraintEnd_toEndOf="parent"
                                 app:tint="#CACACA" />
                           </androidx.constraintlayout.widget.ConstraintLayout>
                           <androidx.viewpager2.widget.ViewPager2
                              android:id="@+id/viewPager"
                              android:layout_width="0dp"
                              android:layout_height="match_parent"
                              android:layout_marginTop="8dp"
                              android:layout_marginBottom="8dp"
                              android:paddingLeft="30dp"
                              android:paddingRight="30dp"
                              app:layout_constraintBottom_toBottomOf="parent"
                              app:layout_constraintEnd_toStartOf="@id/cvRight"
                              app:layout_constraintStart_toEndOf="@id/cvLeft"
                              app:layout_constraintTop_toTopOf="parent"
                              app:layout_constraintHorizontal_weight="1" />
                           <androidx.constraintlayout.widget.ConstraintLayout
                              android:id="@+id/cvRight"
                              android:layout_width="wrap_content"
                              android:layout_height="match_parent"
                              app:layout_constraintBottom_toBottomOf="parent"
                              app:layout_constraintEnd_toEndOf="parent"
                              app:layout_constraintStart_toEndOf="@id/viewPager"
                              app:layout_constraintTop_toTopOf="parent"
                              app:layout_constraintVertical_weight="1">
                              <ImageView
                                 android:layout_width="8dp"
                                 android:layout_height="wrap_content"
                                 android:src="@drawable/arrow_right_round"
                                 android:layout_marginLeft="3dp"
                                 android:layout_marginRight="7dp"
                                 app:layout_constraintBottom_toBottomOf="parent"
                                 app:layout_constraintEnd_toEndOf="parent"
                                 app:layout_constraintTop_toTopOf="parent"
                                 app:layout_constraintStart_toStartOf="parent"
                                 app:tint="#CACACA" />
                           </androidx.constraintlayout.widget.ConstraintLayout>
                        </androidx.constraintlayout.widget.ConstraintLayout>
                     </androidx.cardview.widget.CardView>
                     <androidx.viewpager2.widget.ViewPager2
                        android:id="@+id/viewpager"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginTop="7dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="25dp"
                        app:layout_constraintTop_toBottomOf="@id/cvSlide"
                        app:layout_constraintStart_toStartOf="@id/cvSlide"
                        app:layout_constraintEnd_toEndOf="@id/cvSlide"/>
                     <HorizontalScrollView
                        android:id="@+id/horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="25dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/viewpager"
                        app:layout_constraintBottom_toTopOf="@id/cv1">
                        <me.relex.circleindicator.CircleIndicator3
                            android:id="@+id/indicator"
                            android:layout_width="match_parent"
                            android:layout_height="14dp"
                            android:layout_gravity="center"
                            app:ci_drawable_unselected="@drawable/oval_gray"
                            app:ci_drawable="@drawable/oval_cal_select1"/>
                     </HorizontalScrollView>
                     <View
                        android:id="@+id/view"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        app:layout_constraintTop_toBottomOf="@id/horizontal"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toTopOf="@id/cv1" />
                     <androidx.cardview.widget.CardView
                        android:id="@+id/cv1"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginBottom="12dp"
                        app:cardCornerRadius="5dp"
                        app:cardBackgroundColor="#EEEEEE"
                        app:cardElevation="0dp"
                        app:layout_constraintTop_toBottomOf="@id/view"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent">
                        <androidx.constraintlayout.widget.ConstraintLayout
                           android:layout_width="match_parent"
                           android:layout_height="match_parent">
                           <TextView
                              android:id="@+id/tvCalPct"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:text="순탄수 0%"
                              android:textSize="10dp"
                              android:textColor="@color/black"
                              app:layout_constraintTop_toTopOf="parent"
                              app:layout_constraintStart_toStartOf="parent"
                              app:layout_constraintBottom_toTopOf="@id/tvCar"
                              app:layout_constraintEnd_toStartOf="@id/tvProteinPct"
                              app:layout_constraintVertical_chainStyle="packed" />
                           <TextView
                              android:id="@+id/tvCar"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:text="0.0g"
                              android:textSize="12dp"
                              android:textColor="@color/black"
                              app:layout_constraintTop_toBottomOf="@id/tvCalPct"
                              app:layout_constraintStart_toStartOf="@id/tvCalPct"
                              app:layout_constraintEnd_toEndOf="@id/tvCalPct"
                              app:layout_constraintBottom_toBottomOf="parent"/>
                           <TextView
                              android:id="@+id/tvProteinPct"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:text="단백질 0%"
                              android:textSize="10dp"
                              android:textColor="@color/black"
                              app:layout_constraintTop_toTopOf="parent"
                              app:layout_constraintStart_toEndOf="@id/tvCalPct"
                              app:layout_constraintBottom_toTopOf="@id/tvProtein"
                              app:layout_constraintEnd_toStartOf="@id/tvFatPct"
                              app:layout_constraintVertical_chainStyle="packed"/>
                           <TextView
                              android:id="@+id/tvProtein"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:text="0.0g"
                              android:textSize="12dp"
                              android:textColor="@color/black"
                              app:layout_constraintTop_toBottomOf="@id/tvProteinPct"
                              app:layout_constraintStart_toStartOf="@id/tvProteinPct"
                              app:layout_constraintEnd_toEndOf="@id/tvProteinPct"
                              app:layout_constraintBottom_toBottomOf="parent"/>
                           <TextView
                              android:id="@+id/tvFatPct"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:text="지방 0%"
                              android:textSize="10dp"
                              android:textColor="@color/black"
                              app:layout_constraintTop_toTopOf="parent"
                              app:layout_constraintStart_toEndOf="@id/tvProteinPct"
                              app:layout_constraintBottom_toTopOf="@id/tvFat"
                              app:layout_constraintEnd_toEndOf="parent"
                              app:layout_constraintVertical_chainStyle="packed"/>
                           <TextView
                              android:id="@+id/tvFat"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:text="0.0g"
                              android:textSize="12dp"
                              android:textColor="@color/black"
                              app:layout_constraintTop_toBottomOf="@id/tvFatPct"
                              app:layout_constraintStart_toStartOf="@id/tvFatPct"
                              app:layout_constraintEnd_toEndOf="@id/tvFatPct"
                              app:layout_constraintBottom_toBottomOf="parent"/>
                        </androidx.constraintlayout.widget.ConstraintLayout>
                     </androidx.cardview.widget.CardView>
                  </androidx.constraintlayout.widget.ConstraintLayout>
               </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
         </androidx.constraintlayout.widget.ConstraintLayout>
      </androidx.constraintlayout.widget.ConstraintLayout>
   </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>