<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:app="http://schemas.android.com/apk/res-auto"
   android:id="@+id/mainLayout"
   android:layout_width="match_parent"
   android:layout_height="match_parent"
   android:background="@color/white_black">
   <androidx.constraintlayout.widget.ConstraintLayout
      android:id="@+id/clTop"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent">
      <de.hdodenhof.circleimageview.CircleImageView
         android:id="@+id/ivUser"
         android:layout_width="49dp"
         android:layout_height="49dp"
         android:background="@drawable/oval"
         android:src="@drawable/user_grey"
         android:layout_marginStart="25dp"
         app:layout_constraintTop_toTopOf="@id/cvCalendar"
         app:layout_constraintBottom_toBottomOf="@id/cvCalendar"
         app:layout_constraintStart_toStartOf="parent"/>
      <TextView
         android:id="@+id/tvName"
         android:layout_width="0dp"
         android:layout_height="wrap_content"
         android:layout_marginLeft="6dp"
         android:layout_marginRight="15dp"
         android:text="바디웰 님"
         android:textSize="16dp"
         android:textColor="@color/black_white"
         android:fontFamily="@font/noto"
         android:includeFontPadding="false"
         android:maxLines="1"
         android:ellipsize="end"
         app:layout_constraintStart_toEndOf="@id/ivUser"
         app:layout_constraintTop_toTopOf="@id/ivUser"
         app:layout_constraintBottom_toBottomOf="@id/ivUser"/>
      <androidx.cardview.widget.CardView
         android:id="@+id/cvCalendar"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginTop="15dp"
         android:layout_marginEnd="20dp"
         android:backgroundTint="@color/purple"
         app:cardCornerRadius="12dp"
         android:elevation="0dp"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintEnd_toEndOf="parent">
         <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/btnCalendar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="4dp"
            android:paddingBottom="6dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent">
            <TextView
               android:id="@+id/tvYear"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginLeft="15dp"
               android:text="2023"
               android:textSize="18dp"
               android:textColor="@color/white"
               android:fontFamily="@font/noto"
               android:includeFontPadding="false"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintTop_toTopOf="parent"
               app:layout_constraintBottom_toBottomOf="parent"
               app:layout_constraintEnd_toStartOf="@id/tvMonth"
               app:layout_constraintHorizontal_chainStyle="packed"/>
            <TextView
               android:id="@+id/tvMonth"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginLeft="6dp"
               android:text="8"
               android:textSize="37dp"
               android:fontFamily="@font/noto_bold"
               android:includeFontPadding="false"
               android:textColor="@color/white"
               app:layout_constraintStart_toEndOf="@id/tvYear"
               app:layout_constraintTop_toTopOf="@id/tvYear"
               app:layout_constraintBottom_toBottomOf="@id/tvYear"
               app:layout_constraintEnd_toStartOf="@id/ivDown" />
            <ImageView
               android:id="@+id/ivDown"
               android:layout_width="22dp"
               android:layout_height="22dp"
               android:src="@drawable/arrow_down_main"
               android:layout_marginLeft="8dp"
               android:layout_marginRight="15dp"
               app:layout_constraintStart_toEndOf="@id/tvMonth"
               app:layout_constraintTop_toTopOf="@id/tvYear"
               app:layout_constraintBottom_toBottomOf="@id/tvYear"
               app:layout_constraintEnd_toEndOf="parent"/>
         </androidx.constraintlayout.widget.ConstraintLayout>
      </androidx.cardview.widget.CardView>
   </androidx.constraintlayout.widget.ConstraintLayout>
   <androidx.constraintlayout.widget.ConstraintLayout
      android:id="@+id/clCalendar"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:background="@drawable/rec_15"
      android:backgroundTint="@color/sub_button"
      android:layout_marginTop="15dp"
      android:layout_marginBottom="20dp"
      android:layout_marginLeft="20dp"
      android:layout_marginRight="20dp"
      android:paddingTop="15dp"
      android:paddingBottom="15dp"
      android:paddingLeft="8dp"
      android:paddingRight="8dp"
      app:layout_constraintTop_toBottomOf="@id/clTop"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent">
      <androidx.constraintlayout.widget.ConstraintLayout
         android:id="@+id/calWeek"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintEnd_toEndOf="parent"
         app:layout_constraintBottom_toTopOf="@id/recyclerView"
         app:layout_constraintVertical_chainStyle="packed">
         <TextView
            android:id="@+id/tvSun"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="SUN"
            android:textAlignment="center"
            android:textColor="@color/black_white"
            android:textSize="11dp"
            android:fontFamily="@font/noto"
            android:includeFontPadding="false"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/tvMon"
            app:layout_constraintHorizontal_weight="1"/>
         <TextView
            android:id="@+id/tvMon"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="MON"
            android:textAlignment="center"
            android:textColor="@color/black_white"
            android:textSize="11dp"
            android:fontFamily="@font/noto"
            android:includeFontPadding="false"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/tvSun"
            app:layout_constraintEnd_toStartOf="@+id/tvTue"
            app:layout_constraintHorizontal_weight="1"/>
         <TextView
            android:id="@+id/tvTue"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="TUE"
            android:textAlignment="center"
            android:textColor="@color/black_white"
            android:textSize="11dp"
            android:fontFamily="@font/noto"
            android:includeFontPadding="false"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/tvMon"
            app:layout_constraintEnd_toStartOf="@id/tvWed"
            app:layout_constraintHorizontal_weight="1"/>
         <TextView
            android:id="@+id/tvWed"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="WED"
            android:textAlignment="center"
            android:textColor="@color/black_white"
            android:textSize="11dp"
            android:fontFamily="@font/noto"
            android:includeFontPadding="false"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/tvTue"
            app:layout_constraintEnd_toStartOf="@id/tvThu"
            app:layout_constraintHorizontal_weight="1"/>
         <TextView
            android:id="@+id/tvThu"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="THU"
            android:textAlignment="center"
            android:textColor="@color/black_white"
            android:textSize="11dp"
            android:fontFamily="@font/noto"
            android:includeFontPadding="false"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/tvWed"
            app:layout_constraintEnd_toStartOf="@id/tvFri"
            app:layout_constraintHorizontal_weight="1"/>
         <TextView
            android:id="@+id/tvFri"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="FRI"
            android:textAlignment="center"
            android:textColor="@color/black_white"
            android:textSize="11dp"
            android:fontFamily="@font/noto"
            android:includeFontPadding="false"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/tvThu"
            app:layout_constraintEnd_toStartOf="@id/tvSat"
            app:layout_constraintHorizontal_weight="1"/>
         <TextView
            android:id="@+id/tvSat"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="SAT"
            android:textAlignment="center"
            android:textColor="@color/black_white"
            android:textSize="11dp"
            android:fontFamily="@font/noto"
            android:includeFontPadding="false"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/tvFri"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_weight="1"/>
      </androidx.constraintlayout.widget.ConstraintLayout>
      <androidx.recyclerview.widget.RecyclerView
         android:id="@+id/recyclerView"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:layout_marginTop="4dp"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintEnd_toEndOf="parent"
         app:layout_constraintTop_toBottomOf="@id/calWeek"
         app:layout_constraintBottom_toBottomOf="parent"/>
   </androidx.constraintlayout.widget.ConstraintLayout>
   <androidx.constraintlayout.widget.ConstraintLayout
      android:id="@+id/clFood"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:background="@drawable/rec_15"
      android:backgroundTint="@color/sub_button"
      android:layout_marginTop="15dp"
      android:layout_marginLeft="20dp"
      android:layout_marginRight="20dp"
      android:padding="14dp"
      app:layout_constraintTop_toBottomOf="@id/clCalendar"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent">
      <ImageView
         android:id="@+id/ivFood"
         android:layout_width="14dp"
         android:layout_height="14dp"
         android:src="@drawable/icon_food"
         android:layout_marginLeft="3dp"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintTop_toTopOf="@id/tvFoodTitle"
         app:layout_constraintBottom_toBottomOf="@id/tvFoodTitle"/>
      <TextView
         android:id="@+id/tvFoodTitle"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginLeft="3dp"
         android:text="식단"
         android:textSize="16dp"
         android:textColor="@color/black_white"
         android:fontFamily="@font/noto_bold"
         android:includeFontPadding="false"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintStart_toEndOf="@id/ivFood"
         app:layout_constraintBottom_toBottomOf="parent"/>
      <TextView
         android:id="@+id/tvFoodGoal"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginLeft="7dp"
         android:text="0/0 kcal"
         android:textSize="12dp"
         android:textColor="@color/black_white"
         android:fontFamily="@font/noto"
         android:includeFontPadding="false"
         android:maxLines="1"
         android:ellipsize="end"
         app:layout_constraintTop_toTopOf="@id/ivFood"
         app:layout_constraintStart_toEndOf="@id/tvFoodTitle"
         app:layout_constraintBottom_toBottomOf="@id/ivFood"/>
      <TextView
         android:id="@+id/btnFood"
         android:layout_width="120dp"
         android:layout_height="37dp"
         android:gravity="center"
         android:background="@drawable/rec_30"
         android:backgroundTint="@color/sub_button_round"
         android:text="0% 완료"
         android:textSize="13dp"
         android:textColor="@color/dark_grey_white"
         android:textAlignment="center"
         android:fontFamily="@font/noto_bold"
         android:includeFontPadding="false"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintBottom_toBottomOf="parent"
         app:layout_constraintEnd_toEndOf="parent"/>
   </androidx.constraintlayout.widget.ConstraintLayout>
   <androidx.constraintlayout.widget.ConstraintLayout
      android:id="@+id/clWater"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:background="@drawable/rec_15"
      android:backgroundTint="@color/sub_button"
      android:layout_marginTop="11dp"
      android:layout_marginLeft="20dp"
      android:layout_marginRight="20dp"
      android:padding="14dp"
      app:layout_constraintTop_toBottomOf="@id/clFood"
      app:layout_constraintStart_toEndOf="parent"
      app:layout_constraintEnd_toEndOf="parent">
      <ImageView
         android:id="@+id/ivWater"
         android:layout_width="13dp"
         android:layout_height="13dp"
         android:src="@drawable/icon_water"
         android:layout_marginLeft="3dp"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintBottom_toBottomOf="parent"/>
      <TextView
         android:id="@+id/tvWaterTitle"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginLeft="3dp"
         android:text="수분"
         android:textSize="16dp"
         android:textColor="@color/black_white"
         android:fontFamily="@font/noto_bold"
         android:includeFontPadding="false"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintStart_toEndOf="@id/ivWater"
         app:layout_constraintBottom_toBottomOf="parent"/>
      <TextView
         android:id="@+id/tvWaterGoal"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginLeft="7dp"
         android:text="0/0 잔"
         android:textSize="12dp"
         android:textColor="@color/black_white"
         android:fontFamily="@font/noto"
         android:includeFontPadding="false"
         android:maxLines="1"
         android:ellipsize="end"
         app:layout_constraintTop_toTopOf="@id/ivWater"
         app:layout_constraintStart_toEndOf="@id/tvWaterTitle"
         app:layout_constraintBottom_toBottomOf="@id/ivWater"/>
      <TextView
         android:id="@+id/btnWater"
         android:layout_width="120dp"
         android:layout_height="37dp"
         android:gravity="center"
         android:background="@drawable/rec_30"
         android:backgroundTint="@color/sub_button_round"
         android:text="0% 완료"
         android:textSize="13dp"
         android:textColor="@color/dark_grey_white"
         android:textAlignment="center"
         android:fontFamily="@font/noto_bold"
         android:includeFontPadding="false"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintBottom_toBottomOf="parent"
         app:layout_constraintEnd_toEndOf="parent"/>
   </androidx.constraintlayout.widget.ConstraintLayout>
   <androidx.constraintlayout.widget.ConstraintLayout
      android:id="@+id/clExercise"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:background="@drawable/rec_15"
      android:backgroundTint="@color/sub_button"
      android:layout_marginTop="11dp"
      android:layout_marginLeft="20dp"
      android:layout_marginRight="20dp"
      android:padding="14dp"
      app:layout_constraintTop_toBottomOf="@id/clWater"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent">
      <ImageView
         android:id="@+id/ivExercise"
         android:layout_width="15dp"
         android:layout_height="15dp"
         android:src="@drawable/icon_exercise"
         android:layout_marginStart="3dp"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintBottom_toBottomOf="parent"/>
      <TextView
         android:id="@+id/tvExerciseTitle"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginLeft="3dp"
         android:text="운동"
         android:textSize="16dp"
         android:textColor="@color/black_white"
         android:fontFamily="@font/noto_bold"
         android:includeFontPadding="false"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintStart_toEndOf="@id/ivExercise"
         app:layout_constraintBottom_toBottomOf="parent"/>
      <TextView
         android:id="@+id/tvExerciseGoal"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginLeft="7dp"
         android:text="0/0kcal"
         android:textSize="12dp"
         android:textColor="@color/black_white"
         android:fontFamily="@font/noto"
         android:includeFontPadding="false"
         android:maxLines="1"
         android:ellipsize="end"
         app:layout_constraintTop_toTopOf="@id/ivExercise"
         app:layout_constraintStart_toEndOf="@id/tvExerciseTitle"
         app:layout_constraintBottom_toBottomOf="@id/ivExercise"/>
      <TextView
         android:id="@+id/btnExercise"
         android:layout_width="120dp"
         android:layout_height="37dp"
         android:gravity="center"
         android:background="@drawable/rec_30"
         android:backgroundTint="@color/sub_button_round"
         android:text="0% 완료"
         android:textSize="13dp"
         android:textColor="@color/dark_grey_white"
         android:textAlignment="center"
         android:fontFamily="@font/noto_bold"
         android:includeFontPadding="false"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintBottom_toBottomOf="parent"
         app:layout_constraintEnd_toEndOf="parent"/>
   </androidx.constraintlayout.widget.ConstraintLayout>
   <androidx.constraintlayout.widget.ConstraintLayout
      android:id="@+id/clBody"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:background="@drawable/rec_15"
      android:backgroundTint="@color/sub_button"
      android:layout_marginTop="11dp"
      android:layout_marginLeft="20dp"
      android:layout_marginRight="20dp"
      android:padding="14dp"
      app:layout_constraintTop_toBottomOf="@id/clExercise"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent">
      <ImageView
         android:id="@+id/ivBody"
         android:layout_width="13dp"
         android:layout_height="13dp"
         android:src="@drawable/icon_body"
         android:layout_marginTop="1dp"
         android:layout_marginStart="3dp"
         app:layout_constraintTop_toTopOf="@id/tvBodyTitle"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintBottom_toBottomOf="@id/tvBodyTitle"/>
      <TextView
         android:id="@+id/tvBodyTitle"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginLeft="3dp"
         android:text="신체"
         android:textSize="16dp"
         android:textColor="@color/black_white"
         android:fontFamily="@font/noto_bold"
         android:includeFontPadding="false"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintStart_toEndOf="@id/ivBody"
         app:layout_constraintBottom_toBottomOf="parent"/>
      <TextView
         android:id="@+id/tvBodyGoal"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginLeft="7dp"
         android:text="0/0kg"
         android:textSize="12dp"
         android:textColor="@color/black_white"
         android:fontFamily="@font/noto"
         android:includeFontPadding="false"
         android:maxLines="1"
         android:ellipsize="end"
         app:layout_constraintTop_toTopOf="@id/ivBody"
         app:layout_constraintStart_toEndOf="@id/tvBodyTitle"
         app:layout_constraintBottom_toBottomOf="@id/ivBody"/>
      <TextView
         android:id="@+id/btnBody"
         android:layout_width="120dp"
         android:layout_height="37dp"
         android:gravity="center"
         android:background="@drawable/rec_30"
         android:backgroundTint="@color/sub_button_round"
         android:text="0% 완료"
         android:textAlignment="center"
         android:textColor="@color/dark_grey_white"
         android:textSize="13dp"
         android:fontFamily="@font/noto_bold"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintBottom_toBottomOf="parent"
         app:layout_constraintEnd_toEndOf="parent"/>
   </androidx.constraintlayout.widget.ConstraintLayout>
   <androidx.constraintlayout.widget.ConstraintLayout
      android:id="@+id/clSleep"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:background="@drawable/rec_15"
      android:backgroundTint="@color/sub_button"
      android:layout_marginTop="11dp"
      android:layout_marginLeft="20dp"
      android:layout_marginRight="20dp"
      android:padding="14dp"
      app:layout_constraintTop_toBottomOf="@id/clBody"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent">
      <ImageView
         android:id="@+id/ivSleep"
         android:layout_width="14dp"
         android:layout_height="14dp"
         android:src="@drawable/icon_sleep"
         android:layout_marginStart="3dp"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintBottom_toBottomOf="parent"/>
      <TextView
         android:id="@+id/tvSleepTitle"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginLeft="3dp"
         android:text="수면"
         android:textSize="16dp"
         android:textColor="@color/black_white"
         android:fontFamily="@font/noto_bold"
         android:includeFontPadding="false"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintStart_toEndOf="@id/ivSleep"
         app:layout_constraintBottom_toBottomOf="parent"/>
      <TextView
         android:id="@+id/tvSleepGoal"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginLeft="7dp"
         android:text="0h0m/0h"
         android:textSize="12dp"
         android:textColor="@color/black_white"
         android:fontFamily="@font/noto"
         android:includeFontPadding="false"
         android:maxLines="1"
         android:ellipsize="end"
         app:layout_constraintTop_toTopOf="@id/ivSleep"
         app:layout_constraintStart_toEndOf="@id/tvSleepTitle"
         app:layout_constraintBottom_toBottomOf="@id/ivSleep"/>
      <TextView
         android:id="@+id/btnSleep"
         android:layout_width="120dp"
         android:layout_height="37dp"
         android:gravity="center"
         android:background="@drawable/rec_30"
         android:backgroundTint="@color/sub_button_round"
         android:text="0% 완료"
         android:textSize="13dp"
         android:textColor="@color/dark_grey_white"
         android:textAlignment="center"
         android:fontFamily="@font/noto_bold"
         android:includeFontPadding="false"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintBottom_toBottomOf="parent"
         app:layout_constraintEnd_toEndOf="parent"/>
   </androidx.constraintlayout.widget.ConstraintLayout>
   <androidx.constraintlayout.widget.ConstraintLayout
      android:id="@+id/clMedicine"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:background="@drawable/rec_15"
      android:backgroundTint="@color/sub_button"
      android:layout_marginTop="11dp"
      android:layout_marginLeft="20dp"
      android:layout_marginRight="20dp"
      android:padding="14dp"
      app:layout_constraintTop_toBottomOf="@id/clSleep"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent">
      <ImageView
         android:id="@+id/ivMedicine"
         android:layout_width="13dp"
         android:layout_height="13dp"
         android:src="@drawable/icon_medicine"
         android:layout_marginLeft="3dp"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintBottom_toBottomOf="parent"/>
      <TextView
         android:id="@+id/tvMedicineTitle"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginLeft="3dp"
         android:text="약복용"
         android:textSize="16dp"
         android:textColor="@color/black_white"
         android:fontFamily="@font/noto_bold"
