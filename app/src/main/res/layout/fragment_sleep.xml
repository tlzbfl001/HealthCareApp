<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:app="http://schemas.android.com/apk/res-auto"
   android:layout_width="match_parent"
   android:layout_height="match_parent"
   android:orientation="vertical"
   android:background="@color/white">
   <ImageView
      android:layout_width="145dp"
      android:layout_height="145dp"
      android:background="@drawable/sleep_top"
      android:alpha="0.9"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintStart_toStartOf="parent"/>
   <androidx.constraintlayout.widget.ConstraintLayout
       android:id="@+id/clBack"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:paddingTop="30dp"
       android:paddingLeft="25dp"
       android:paddingBottom="10dp"
       android:paddingRight="10dp"
       app:layout_constraintTop_toTopOf="parent"
       app:layout_constraintStart_toStartOf="parent">
      <ImageView
          android:layout_width="15dp"
          android:layout_height="25dp"
          android:src="@drawable/arrow_left"
          app:layout_constraintTop_toTopOf="parent"
          app:layout_constraintStart_toStartOf="parent"/>
   </androidx.constraintlayout.widget.ConstraintLayout>
   <ImageView
      android:id="@+id/ivLogo"
      android:layout_width="120dp"
      android:layout_height="13dp"
      android:src="@drawable/logo3"
      android:layout_marginTop="30dp"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent"/>
   <androidx.cardview.widget.CardView
      android:id="@+id/cvDate"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="17dp"
      app:cardBackgroundColor="#FBFAFA"
      app:cardCornerRadius="15dp"
      app:cardElevation="0dp"
      app:layout_constraintTop_toBottomOf="@id/ivLogo"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent">
      <androidx.constraintlayout.widget.ConstraintLayout
         android:layout_width="wrap_content"
         android:layout_height="wrap_content">
         <TextView
            android:id="@+id/leftArrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_left_triangle"
            android:layout_marginLeft="7dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/tv1"/>
         <TextView
            android:id="@+id/tv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2023년 7월 23일"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/rightArrow"
            app:layout_constraintStart_toEndOf="@id/leftArrow"/>
         <TextView
            android:id="@+id/rightArrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_right_triangle"
            android:layout_marginRight="7dp"
            app:layout_constraintStart_toEndOf="@id/tv1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
      </androidx.constraintlayout.widget.ConstraintLayout>
   </androidx.cardview.widget.CardView>
   <androidx.constraintlayout.widget.ConstraintLayout
      android:id="@+id/cl1"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      app:layout_constraintTop_toBottomOf="@id/cvDate"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent">
      <androidx.cardview.widget.CardView
         android:id="@+id/cvFood"
         android:layout_width="0dp"
         android:layout_height="wrap_content"
         android:layout_marginTop="17dp"
         android:layout_marginBottom="17dp"
         android:layout_marginRight="8dp"
         android:layout_marginLeft="25dp"
         app:cardBackgroundColor="@color/white"
         app:cardCornerRadius="6dp"
         app:cardElevation="3dp"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintBottom_toBottomOf="parent"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintEnd_toStartOf="@id/cvWater"
         app:layout_constraintHorizontal_weight="3">
         <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:paddingTop="6dp"
            android:paddingBottom="6dp"
            android:text="식단"
            android:textSize="18dp"
            android:textColor="#888888"/>
      </androidx.cardview.widget.CardView>
      <androidx.cardview.widget.CardView
         android:id="@+id/cvWater"
         android:layout_width="0dp"
         android:layout_height="wrap_content"
         android:layout_marginTop="17dp"
         android:layout_marginBottom="17dp"
         android:layout_marginRight="8dp"
         app:cardBackgroundColor="@color/white"
         app:cardCornerRadius="6dp"
         app:cardElevation="3dp"
         app:layout_constraintStart_toEndOf="@id/cvFood"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintBottom_toBottomOf="parent"
         app:layout_constraintEnd_toStartOf="@id/cvExercise"
         app:layout_constraintHorizontal_weight="2">
         <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:paddingTop="6dp"
            android:paddingBottom="6dp"
            android:text="물"
            android:textSize="18dp"
            android:textColor="#888888"/>
      </androidx.cardview.widget.CardView>
      <androidx.cardview.widget.CardView
         android:id="@+id/cvExercise"
         android:layout_width="0dp"
         android:layout_height="wrap_content"
         android:layout_marginTop="17dp"
         android:layout_marginBottom="17dp"
         android:layout_marginRight="8dp"
         app:cardBackgroundColor="@color/white"
         app:cardCornerRadius="6dp"
         app:cardElevation="3dp"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintBottom_toBottomOf="parent"
         app:layout_constraintStart_toEndOf="@id/cvWater"
         app:layout_constraintEnd_toStartOf="@id/cvBody"
         app:layout_constraintHorizontal_weight="3">
         <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:paddingTop="6dp"
            android:paddingBottom="6dp"
            android:text="운동"
            android:textSize="18dp"
            android:textColor="#888888"/>
      </androidx.cardview.widget.CardView>
      <androidx.cardview.widget.CardView
         android:id="@+id/cvBody"
         android:layout_width="0dp"
         android:layout_height="wrap_content"
         android:layout_marginTop="17dp"
         android:layout_marginBottom="17dp"
         android:layout_marginRight="8dp"
         app:cardBackgroundColor="@color/white"
         app:cardCornerRadius="6dp"
         app:cardElevation="3dp"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintBottom_toBottomOf="parent"
         app:layout_constraintStart_toEndOf="@id/cvExercise"
         app:layout_constraintEnd_toStartOf="@id/cvSleep"
         app:layout_constraintHorizontal_weight="3">
         <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:paddingTop="6dp"
            android:paddingBottom="6dp"
            android:text="신체"
            android:textSize="18dp"
            android:textColor="#888888"/>
      </androidx.cardview.widget.CardView>
      <androidx.cardview.widget.CardView
         android:id="@+id/cvSleep"
         android:layout_width="0dp"
         android:layout_height="wrap_content"
         android:layout_marginTop="17dp"
         android:layout_marginBottom="17dp"
         android:layout_marginRight="8dp"
         app:cardBackgroundColor="#4E698A"
         app:cardCornerRadius="6dp"
         app:cardElevation="3dp"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintBottom_toBottomOf="parent"
         app:layout_constraintStart_toEndOf="@id/cvBody"
         app:layout_constraintEnd_toStartOf="@id/cvDrug"
         app:layout_constraintHorizontal_weight="3">
         <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:paddingTop="6dp"
            android:paddingBottom="6dp"
            android:text="수면"
            android:textSize="18dp"
            android:textColor="@color/white"/>
      </androidx.cardview.widget.CardView>
      <androidx.cardview.widget.CardView
         android:id="@+id/cvDrug"
         android:layout_width="0dp"
         android:layout_height="wrap_content"
         android:layout_marginTop="17dp"
         android:layout_marginBottom="17dp"
         android:layout_marginRight="25dp"
         app:cardBackgroundColor="@color/white"
         app:cardCornerRadius="6dp"
         app:cardElevation="3dp"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintBottom_toBottomOf="parent"
         app:layout_constraintStart_toEndOf="@id/cvSleep"
         app:layout_constraintEnd_toEndOf="parent"
         app:layout_constraintHorizontal_weight="4">
         <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:paddingTop="6dp"
            android:paddingBottom="6dp"
            android:text="약복용"
            android:textSize="18dp"
            android:textColor="#888888"/>
      </androidx.cardview.widget.CardView>
   </androidx.constraintlayout.widget.ConstraintLayout>
   <androidx.constraintlayout.widget.ConstraintLayout
      android:id="@+id/cl2"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      app:layout_constraintTop_toBottomOf="@id/cl1"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent">
      <androidx.constraintlayout.widget.ConstraintLayout
         android:id="@+id/clLeft"
         android:layout_width="0dp"
         android:layout_height="wrap_content"
         android:layout_marginLeft="25dp"
         android:background="@drawable/rec_detail"
         android:paddingTop="25dp"
         android:paddingBottom="25dp"
         app:layout_constraintEnd_toStartOf="@id/clRight"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintHorizontal_weight="5">
         <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="120dp"
            android:layout_height="120dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <com.dinuscxj.progressbar.CircleProgressBar
               android:id="@+id/pbSleep"
               android:layout_width="match_parent"
               android:layout_height="match_parent"
               app:progress_background_color="#E1E5E5"
               app:progress_end_color="#4E698A"
               app:progress_start_color="#4E698A"
               app:progress_start_degree="90"
               app:progress_stroke_cap="round"
               app:progress_stroke_width="12dp"
               app:progress_style="solid_line" />
            <androidx.constraintlayout.widget.ConstraintLayout
               android:layout_width="match_parent"
               android:layout_height="match_parent"
               android:layout_margin="18dp"
               android:background="@drawable/oval_border_sleep">
               <TextView
                  android:id="@+id/tvSleep1"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="수면 시간"
                  android:textColor="@color/black"
                  android:textSize="12dp"
                  app:layout_constraintBottom_toTopOf="@id/tvSleep2"
                  app:layout_constraintEnd_toEndOf="parent"
                  app:layout_constraintStart_toStartOf="parent"
                  app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintVertical_chainStyle="packed" />
               <TextView
                  android:id="@+id/tvSleep2"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="0"
                  android:textColor="@color/black"
                  android:textSize="12dp"
                  app:layout_constraintBottom_toBottomOf="parent"
                  app:layout_constraintEnd_toEndOf="parent"
                  app:layout_constraintStart_toStartOf="parent"
                  app:layout_constraintTop_toBottomOf="@id/tvSleep1" />
            </androidx.constraintlayout.widget.ConstraintLayout>
         </androidx.constraintlayout.widget.ConstraintLayout>
      </androidx.constraintlayout.widget.ConstraintLayout>
      <androidx.constraintlayout.widget.ConstraintLayout
         android:id="@+id/clRight"
         android:layout_width="0dp"
         android:layout_height="wrap_content"
         android:layout_marginRight="25dp"
         app:layout_constraintTop_toTopOf="@id/clLeft"
         app:layout_constraintEnd_toEndOf="parent"
         app:layout_constraintStart_toEndOf="@id/clLeft"
         app:layout_constraintHorizontal_weight="3">
         <androidx.cardview.widget.CardView
            android:id="@+id/cvGoal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginRight="5dp"
            app:cardBackgroundColor="#F2F2F2"
            app:cardCornerRadius="8dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent">
            <androidx.constraintlayout.widget.ConstraintLayout
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:paddingTop="5dp"
               android:paddingBottom="5dp"
               android:paddingLeft="10dp"
               android:paddingRight="10dp">
               <ImageView
                  android:id="@+id/iv1"
                  android:layout_width="11dp"
                  android:layout_height="14dp"
                  android:src="@drawable/setting_goal"
                  app:tint="@color/black"
                  app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintStart_toStartOf="parent"
                  app:layout_constraintBottom_toBottomOf="parent"/>
               <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_gravity="center"
                  android:paddingLeft="5dp"
                  android:text="목표설정"
                  android:textColor="@color/black"
                  android:textSize="14dp"
                  app:layout_constraintStart_toEndOf="@id/iv1"
                  app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintEnd_toEndOf="parent"
                  app:layout_constraintBottom_toBottomOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
         </androidx.cardview.widget.CardView>
         <TextView
            android:id="@+id/tv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:text="목표 수면 시간"
            android:textColor="#999999"
            android:textSize="12dp"
            app:layout_constraintTop_toBottomOf="@id/cvGoal"
            app:layout_constraintEnd_toEndOf="@id/cvGoal"/>
         <TextView
            android:id="@+id/tv3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textColor="@color/black"
            android:textSize="18dp"
            app:layout_constraintTop_toBottomOf="@id/tv2"
            app:layout_constraintEnd_toEndOf="@id/cvGoal"/>
         <TextView
            android:id="@+id/tv4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="남은 수면 시간"
            android:textColor="#999999"
            android:textSize="12dp"
            app:layout_constraintTop_toBottomOf="@id/tv3"
            app:layout_constraintEnd_toEndOf="@id/cvGoal" />
         <TextView
            android:id="@+id/tv5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textColor="@color/black"
            android:textSize="18dp"
            app:layout_constraintTop_toBottomOf="@id/tv4"
            app:layout_constraintEnd_toEndOf="@id/cvGoal" />
      </androidx.constraintlayout.widget.ConstraintLayout>
   </androidx.constraintlayout.widget.ConstraintLayout>
   <androidx.constraintlayout.widget.ConstraintLayout
      android:layout_width="match_parent"
      android:layout_height="0dp"
      app:layout_constraintTop_toBottomOf="@id/cl2"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintVertical_weight="1">
      <TextView
         android:id="@+id/tv6"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginLeft="25dp"
         android:text="하루 수면"
         android:textColor="#777777"
         android:textSize="16dp"
         app:layout_constraintTop_toTopOf="@id/clRecord"
         app:layout_constraintBottom_toBottomOf="@id/clRecord"
         app:layout_constraintStart_toStartOf="parent"/>
      <androidx.constraintlayout.widget.ConstraintLayout
         android:id="@+id/clRecord"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:background="@drawable/detail_bar"
         android:backgroundTint="#4E698A"
         android:elevation="5dp"
         android:layout_marginTop="20dp"
         android:paddingLeft="7dp"
         android:paddingTop="5dp"
         android:paddingBottom="5dp"
         android:paddingRight="50dp"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintEnd_toEndOf="parent">
         <ImageView
            android:id="@+id/iv2"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:background="@drawable/oval"
            android:src="@drawable/pencil"
            android:padding="3.3dp"
            app:tint="#4E698A"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" />
         <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:text="수면 기록"
            android:textColor="@color/white"
            android:textSize="14dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/iv2"/>
      </androidx.constraintlayout.widget.ConstraintLayout>
      <androidx.constraintlayout.widget.ConstraintLayout
         android:id="@+id/cl3"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:layout_marginTop="15dp"
         android:layout_marginLeft="25dp"
         android:layout_marginRight="25dp"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintEnd_toEndOf="parent"
         app:layout_constraintTop_toBottomOf="@id/clRecord">
         <androidx.cardview.widget.CardView
            android:id="@+id/cv1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#5E66A3"
            android:layout_marginRight="11dp"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="7dp"
            app:layout_constraintEnd_toStartOf="@id/cv2"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_weight="1">
            <androidx.constraintlayout.widget.ConstraintLayout
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:padding="8dp">
               <TextView
                  android:id="@+id/tvAverage1"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="평균"
                  android:textSize="12dp"
                  android:textColor="@color/white"
                  app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintStart_toStartOf="parent"/>
               <ImageView
                  android:layout_width="20dp"
                  android:layout_height="20dp"
                  android:src="@drawable/sleeping"
                  app:layout_constraintEnd_toEndOf="parent"
                  app:layout_constraintTop_toTopOf="parent"/>
               <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_marginTop="7dp"
                  android:layout_marginRight="10dp"
                  android:text="취침 시간"
                  android:textSize="14dp"
                  android:textColor="@color/white"
                  app:layout_constraintTop_toBottomOf="@id/tvAverage1"
                  app:layout_constraintStart_toStartOf="parent"
                  app:layout_constraintBottom_toBottomOf="parent"/>
               <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="0"
                  android:textSize="14dp"
                  android:textColor="@color/white"
                  app:layout_constraintEnd_toEndOf="parent"
                  app:layout_constraintBottom_toBottomOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
         </androidx.cardview.widget.CardView>
         <androidx.cardview.widget.CardView
            android:id="@+id/cv2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#5E66A3"
            android:layout_marginLeft="11dp"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="7dp"
            app:layout_constraintStart_toEndOf="@id/cv1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_weight="1">
            <androidx.constraintlayout.widget.ConstraintLayout
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:padding="7dp">
               <TextView
                  android:id="@+id/tvAverage2"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="평균"
                  android:textSize="12dp"
                  android:textColor="@color/white"
                  app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintStart_toStartOf="parent"/>
               <ImageView
                  android:layout_width="20dp"
                  android:layout_height="20dp"
                  android:src="@drawable/sunny"
                  app:layout_constraintEnd_toEndOf="parent"
                  app:layout_constraintTop_toTopOf="parent"/>
               <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_marginTop="7dp"
                  android:layout_marginRight="10dp"
                  android:text="취침 시간"
                  android:textSize="14dp"
                  android:textColor="@color/white"
                  app:layout_constraintTop_toBottomOf="@id/tvAverage2"
                  app:layout_constraintStart_toStartOf="parent"
                  app:layout_constraintBottom_toBottomOf="parent"/>
               <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="0"
                  android:textSize="14dp"
                  android:textColor="@color/white"
                  app:layout_constraintEnd_toEndOf="parent"
                  app:layout_constraintBottom_toBottomOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
         </androidx.cardview.widget.CardView>
      </androidx.constraintlayout.widget.ConstraintLayout>
      <androidx.constraintlayout.widget.ConstraintLayout
         android:id="@+id/cl4"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:layout_marginTop="10dp"
         android:layout_marginLeft="25dp"
         android:layout_marginRight="25dp"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintTop_toBottomOf="@id/cl3">
         <androidx.cardview.widget.CardView
            android:id="@+id/cv3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#CC3C78CF"
            android:layout_marginRight="11dp"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="7dp"
            app:layout_constraintEnd_toStartOf="@id/cv4"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_weight="3">
            <androidx.constraintlayout.widget.ConstraintLayout
               android:layout_width="match_parent"
               android:layout_height="35dp"
               android:paddingLeft="8dp"
               android:paddingRight="8dp">
               <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="수면의 질"
                  android:textSize="14dp"
                  android:textColor="@color/white"
                  app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintStart_toStartOf="parent"
                  app:layout_constraintBottom_toBottomOf="parent"/>
               <TextView
                  android:layout_width="wrap_content"
