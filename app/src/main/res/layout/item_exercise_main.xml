<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:app="http://schemas.android.com/apk/res-auto"
   android:layout_width="match_parent"
   android:layout_height="wrap_content"
   android:background="@color/transparent">
   <androidx.cardview.widget.CardView
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginTop="7dp"
      android:layout_marginBottom="7dp"
      android:layout_marginLeft="9dp"
      android:layout_marginRight="9dp"
      app:cardElevation="3dp"
      app:cardCornerRadius="10dp"
      app:cardBackgroundColor="@color/white"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintBottom_toBottomOf="parent">
      <androidx.constraintlayout.widget.ConstraintLayout
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:padding="10dp">
         <TextView
            android:id="@+id/tvCategory"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="무산소"
            android:textColor="#DDDDDD"
            android:textSize="12dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
         <TextView
            android:id="@+id/tvName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="desc"
            android:textColor="#000000"
            android:textSize="14dp"
            app:layout_constraintTop_toTopOf="@id/tvCategory"
            app:layout_constraintBottom_toBottomOf="@id/tvCategory"
            app:layout_constraintEnd_toEndOf="parent"/>
         <TextView
            android:id="@+id/tvTitle2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="운동시간"
            android:textColor="#DDDDDD"
            android:textSize="12dp"
            app:layout_constraintTop_toBottomOf="@id/tvCategory"
            app:layout_constraintStart_toStartOf="parent"/>
         <TextView
            android:id="@+id/tvWorkoutTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="desc"
            android:textColor="#000000"
            android:textSize="14dp"
            app:layout_constraintTop_toTopOf="@id/tvTitle2"
            app:layout_constraintBottom_toBottomOf="@id/tvTitle2"
            app:layout_constraintEnd_toEndOf="parent"/>
         <TextView
            android:id="@+id/tvTitle3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="운동거리"
            android:textColor="#DDDDDD"
            android:textSize="12dp"
            app:layout_constraintTop_toBottomOf="@id/tvTitle2"
            app:layout_constraintStart_toStartOf="parent"/>
         <TextView
            android:id="@+id/tvDistance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="desc"
            android:textColor="#000000"
            android:textSize="14dp"
            app:layout_constraintTop_toTopOf="@id/tvTitle3"
            app:layout_constraintBottom_toBottomOf="@id/tvTitle3"
            app:layout_constraintEnd_toEndOf="parent"/>
         <TextView
            android:id="@+id/tvTitle4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="소모칼로리"
            android:textColor="#DDDDDD"
            android:textSize="12dp"
            app:layout_constraintTop_toBottomOf="@id/tvTitle3"
            app:layout_constraintStart_toStartOf="parent"/>
         <TextView
            android:id="@+id/tvKcal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="desc"
            android:textColor="#000000"
            android:textSize="14dp"
            app:layout_constraintTop_toTopOf="@id/tvTitle4"
            app:layout_constraintBottom_toBottomOf="@id/tvTitle4"
            app:layout_constraintEnd_toEndOf="parent"/>
      </androidx.constraintlayout.widget.ConstraintLayout>
   </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>