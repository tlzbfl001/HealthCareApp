<resources xmlns:tools="http://schemas.android.com/tools">
   <style name="Theme.BodyWell" parent="Theme.MaterialComponents.Light.NoActionBar">
      <item name="android:windowDisablePreview">true</item>
   </style>

   <style name="timePickerStyle" parent="Theme.AppCompat.Dialog.Alert">
      <item name="title">false</item>
      <item name="android:textColorPrimary">#000000</item>
      <item name="android:textColorSecondary">#eeeeee</item>
      <item name="android:textSize">14dp</item>
   </style>

   <style name="BottomNavigationView" parent="@style/TextAppearance.AppCompat.Caption">
      <item name="android:textSize">12dp</item>
   </style>
   <style name="BottomNavigationView.Active" parent="@style/TextAppearance.AppCompat.Caption">
      <item name="android:textSize">13dp</item>
   </style>

   <style name="AlertDialogStyle" parent="Theme.AppCompat.Light.Dialog">
      <item name="colorAccent">@color/black</item>
      <item name="android:windowMinWidthMajor">@dimen/dialog_min_width_major</item>
      <item name="android:windowMinWidthMinor">@dimen/dialog_min_width_minor</item>
   </style>

   <style name="BottomSheetDialogTheme" parent="Theme.Design.Light.BottomSheetDialog">
      <item name="bottomSheetStyle">@style/BottomSheetStyle</item>
   </style>

   <style name="BottomSheetStyle" parent="Widget.Design.BottomSheet.Modal">
      <item name="android:background">@android:color/transparent</item>
   </style>
</resources>